ACTION DEFAULTS
ACTION notificationpushed (DEFAULTVIEW=NO)
END
LAYOUT (TEXT="Push client")
GRID
{
[l1                     ]
[l2   |tmh      |l3|tmp ]
[l4     |nts            ]
[l5     |unm            ]
[b1         |b2         ]
[l6                     ]
[rt                     ]
[                       ]
[                       ]
[l7                     ]
[nl                     ]
[                       ]
[                       ]
}
END
END
ATTRIBUTES
LABEL l1: label1, TEXT="Token maintainer";
LABEL l2: label2, TEXT="Host:";
LABEL l3: label3, TEXT="Port:";
LABEL l4: label4, TEXT="Solution:";
LABEL l5: label5, TEXT="User:";
LABEL l6: label6, TEXT="Token:";
LABEL l7: label7, TEXT="Notifications:";
EDIT tmh = FORMONLY.tm_host, SCROLL;
EDIT tmp = FORMONLY.tm_port;
EDIT nts = FORMONLY.notification_type, NOENTRY;
EDIT unm = FORMONLY.user_name;
BUTTON b1: register, TEXT="Register";
BUTTON b2: unregister, TEXT="Unregister";
TEXTEDIT rt = FORMONLY.registration_token, NOENTRY;
TEXTEDIT nl = FORMONLY.notifications, NOENTRY, STRETCH=BOTH;
END
